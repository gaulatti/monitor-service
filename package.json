{
  "name": "@monitor/service",
  "version": "0.0.1",
  "description": "",
  "author": {
    "name": "Javier Godoy",
    "email": "javier@oxozon.io",
    "url": "https://gaulatti.com/"
  },
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build && tsc --project infrastructure/tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"infrastructure/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test,infrastructure}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:infrastructure": "jest --config ./infrastructure/jest.config.js",
    "db:sync": "npm run build && node dist/utils/sync.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "db:seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "cdk": "cdk",
    "cdk:diff": "cdk diff",
    "cdk:synth": "npm run build && cdk synth",
    "cdk:deploy": "npm run build && cdk deploy --require-approval never"
  },
  "dependencies": {
    "@atproto/api": "^0.13.31",
    "@aws-sdk/client-cloudwatch": "^3.758.0",
    "@aws-sdk/client-dynamodb": "^3.734.0",
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/client-secrets-manager": "^3.758.0",
    "@aws-sdk/util-dynamodb": "^3.734.0",
    "@google/generative-ai": "^0.21.0",
    "@grpc/grpc-js": "^1.12.6",
    "@grpc/proto-loader": "^0.7.13",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.0.11",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-fastify": "^11.0.11",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/sequelize": "^11.0.0",
    "@qdrant/js-client-rest": "^1.15.0",
    "aws-cdk-lib": "2.177.0",
    "axios": "^1.9.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "constructs": "^10.0.0",
    "input": "^1.0.1",
    "jwks-rsa": "^3.1.0",
    "mysql2": "^3.13.0",
    "nanoid": "^3.3.8",
    "node-apn": "^3.0.0",
    "passport-jwt": "^4.0.1",
    "puppeteer-core": "^24.1.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "telegram": "^2.26.22"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@sparticuz/chromium": "^131.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "aws-cdk": "2.177.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
